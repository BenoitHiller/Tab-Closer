{ 
  "name": "tab-closer",
  "version": "1.2.2",
  "manifest_version": 2,
  "description": "Close some of your extra tabs",
  "incognito": "spanning",
  "icons": {
    "16": "trash-16.png",
    "48": "trash-48.png",
    "128": "trash-128-store.png" 
  },
  "browser_action": {
    "default_icon": "trash-16.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "omnibox": {"keyword" : "c"},
  "permissions": [
    "tabs"
  ]
}
